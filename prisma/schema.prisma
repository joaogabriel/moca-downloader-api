// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Download {
  id        Int      @default(autoincrement()) @id
  createdAt DateTime @default(now())
  url       String
  filename  String
  size      Int
  type      String
}
